<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="CamListPreference">
        <attr name="key" format="string" />
        <attr name="title" format="string" />
        <attr name="icon" format="reference" />
        <attr name="defaultValue" format="string|reference" />
        <attr name="entries" format="reference" />
        <attr name="entryValues" format="reference" />
        <attr name="entryIcons" format="reference" />
    </declare-styleable>
    <declare-styleable name="ExpandableImageTextView">
        <!--       最大行数-->
        <attr name="expand_max_line" format="integer"/>
        <!--       展开按钮与文本的间距  默认是10dp-->
        <attr name="expand_space" format="dimension" />
        <attr name="expand_drawable" format="reference"/>
        <attr name="enfold_drawable" format="reference"/>
        <attr name="show_enfold_icon" format="boolean"/>
        <attr name="alertLayout" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="SwipeFlingAdapterView">
        <attr name="y_offset_step" format="dimension" />
        <attr name="rotation_degrees" format="float" />
        <attr name="min_adapter_stack" format="integer" />
        <attr name="max_visible" format="integer" />
    </declare-styleable>
</resources>